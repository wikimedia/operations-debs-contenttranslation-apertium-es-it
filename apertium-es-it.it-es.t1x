                  <clip pos="1" side="tl" part="nbr_sp"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </lu>
              </chunk>
              <b pos="1"/>
            </out>
            <call-macro n="firstWord">
              <with-param pos="2"/>
            </call-macro>
            <call-macro n="f_concord1">
              <with-param pos="2"/>
            </call-macro>
            <out>
