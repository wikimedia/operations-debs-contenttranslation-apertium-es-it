    <def-macro n="tipus_verb" npar="1">
      <choose>
	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="v_amb_a"/>
	    </in>
	  </test>
	  <let>
	    <var n="tipusverb"/>
	    <lit-tag v="vba"/>
	  </let>
	</when>
	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="v_amb_en"/>
	    </in>
	  </test>
	  <let>
	    <var n="tipusverb"/>
	    <lit-tag v="vben"/>
	  </let>
	</when>
	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="v_no_part"/>
	    </in>
	  </test>
	  <let>
