	<pattern-item n="cnjcoo"/>
      </pattern>
      <action>
	<call-macro n="firstWord">
	  <with-param pos="1"/>
	</call-macro>
	<out>
	  <chunk name="cnjcoo" case="caseFirstWord">
	    <tags>
	      <tag><lit-tag v="CC"/></tag>
	    </tags>
	    <lu>   
	      <clip pos="1" side="tl" part="whole"/> 
	    </lu> 
	  </chunk>
	</out>
      </action>
    </rule>

